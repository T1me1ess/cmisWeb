<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.inspur.cmis.mapper.ParamMapper" >

  <!-- 根据参数代码参数类型代码查询参数名称 -->
  <select id="getParamName" parameterType="java.util.Map" resultType="String">
   select paramName from yc_params where paramType=#{paramType} and paramCode=#{paramCode}
  </select>
 <!-- 根据参数类型代码获取对应的参数信息 -->
  <select id="getParamList" parameterType="String" resultType="param">
     select * from yc_params where paramType=#{paramType}
  </select>
  
</mapper>